<?xml version='1.0' encoding='utf-8'?>
<odoo>  
    <data>
        <record id="reject_petty_cash_wizard_form" model="ir.ui.view">
            <field name="name">Reject Petty Cash</field>
            <field name="model">reject.pettycash.wizard</field>
            <field name="arch" type="xml">
                <form string="Reject Petty Cash">
                    <group>
                        <field name="payment_request_id" invisible="1"/>
                        <field name="reject_reason"/>
                    </group>
                    <footer>                        
                        <button name="action_reject" string="Reject" type="object"
                                class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>          
    </data>
</odoo>