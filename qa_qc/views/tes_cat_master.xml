            parent="tes_pengujian_qc_master_menu"/>

        <menuitem
            id="cek_point_master_menu"
            name="Cek Point"
            action="cek_point_master_action"
            parent="tes_pengujian_qc_master_menu"/>
    </data>
    

</odoo>
