        <record id="cek_point_master_view_tree" model="ir.ui.view">
            <field name="name">cek.point.view.tree</field>
            <field name="model">cek.point</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" />
                </tree>
            </field>
        </record>

         <record id="cek_point_master_view_form" model="ir.ui.view">
            <field name="name">cek.point.view.form</field>
            <field name="model">cek.point</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                        <notebook>
                            <page name="komponen" string="Point Pemeriksaan">
                               <field name="cek_point_line_ids">
                                    <tree editable="bottom">
                                        <field name="name"/>
                                        <field name="sequence" widget="handle"/>
                                        <button name="action_show_details" type="object" icon="fa-list" width="0.1" title="Details"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cek_point_master_action" model="ir.actions.act_window">
            <field name="name">Cek Point</field>
            <field name="res_model">cek.point</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                  Cek point not found
                </p><p>
                  Cek point not found
                </p>    
            </field>
        </record>

        <record id="point_pemeriksaan_detail" model="ir.ui.view">
            <field name="name">cek.point.line.form</field>
            <field name="model">cek.point.line</field>
            <field name="priority">1000</field>
            <field name="arch" type="xml">
                <form string="Point Pemeriksaan Details">
                    <group>
                        <group>
                            <field name="name" />
                            <field name="max_line" invisible="1"/>
                            <field name="cek_point_id" invisible="1" />
                            <field name="sequence" invisible="1"/>
                        </group>
                    </group>
                    <field name="point_pemeriksaan_ids" >
                        <tree editable="bottom">
                            <field name="max_line" invisible="1"/>
                            <field name="sequence" widget="handle"/>
                            <field name="cek_point_id" invisible="1" />
                            <field name="cek_point_line_id" invisible="1" />
                            <field name="name"/>
                        </tree>
                    </field>
                </form>
            </field>
        </record>


        <menuitem
            id="tes_pengujian_qc_master_menu"
            name="Master Pengujian QC"
            parent="qa_qc.menu_qc_configuration"/>

        <menuitem
            id="tes_cat_master_menu"
            name="Location Test"
            action="tes_cat_master_action"
            parent="tes_pengujian_qc_master_menu"/>

        <menuitem
            id="jenis_pengujian_master_menu"
            name="Jenis Pengujian"
            action="jenis_pengujian_action"
            parent="tes_pengujian_qc_master_menu"/>

        <menuitem
            id="tahapan_finishing_master_menu"
            name="Tahapan Finishing"
            action="tahapan_finishing_master_action"
