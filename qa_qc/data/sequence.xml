<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_quality_check" model="ir.sequence">
            <field name="name">Quality Check Sequence</field>
            <field name="code">quality.check.seq</field>
            <field name="active">TRUE</field>
            <field name="prefix">QC-</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_inspection_tag_card" model="ir.sequence">
            <field name="name">Inspection Tag Card Sequence</field>
            <field name="code">inspection.tag.card</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="active">TRUE</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_kekuatan_cat" model="ir.sequence">
            <field name="name">Tes Kekuatan Cat</field>
            <field name="code">tes.kakuatan.cek.seq</field>
            <field name="active">TRUE</field>
            <field name="suffix">/QA/ITL/%(month)s/%(y)s</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
            <field name="padding">2</field>
        </record>

        <record id="seq_tes_rebus_tes" model="ir.sequence">
            <field name="name">Tes Rebus Sequence</field>
            <field name="code">tes.rebus.seq.tes</field>
            <field name="suffix">/ITL/QA/%(month)s/%(y)s</field>
            <field name="active">TRUE</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
            <field name="padding">2</field>
        </record>

        <record id="seq_tes_kontruksi" model="ir.sequence">
		   <field name="name">Tes Kontruksi Sequence</field>
		   <field name="code">tes.kontruksi</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">/QA/ITL/%(month)s/%(y)s</field>
		   <field name="padding">2</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>

		<record id="seq_tes_packaging" model="ir.sequence">
		   <field name="name">Tes Packaging Sequence</field>
		   <field name="code">tes.packaging</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">/QA/ITL/%(month)s/%(y)s</field>
		   <field name="padding">2</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>
        
		<record id="seq_tes_vacum" model="ir.sequence">
		   <field name="name">Tes Vacum Sequence</field>
		   <field name="code">tes.vacum</field>
		   <field name="active">TRUE</field>
		   <field name="prefix">/ITL/QA/%(month)s/%(y)s</field>
		   <field name="padding">2</field>
		   <field name="number_next">1</field>
		   <field name="number_increment">1</field>
		</record>
    </data>
</odoo>
