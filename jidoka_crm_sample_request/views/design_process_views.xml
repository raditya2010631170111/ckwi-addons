<?xml version='1.0' encoding='utf-8'?>
<odoo>
    
    <!-- design.process inherit form view -->
    <record id="design_process_form_inherit" model="ir.ui.view">
        <field name="name">design.process.inherit.view.form</field>
        <field name="model">design.process</field>
        <field name="inherit_id" ref="design_process.design_process_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='deadline']" position="after">
                <field name="sample_request_id"/>
            </xpath>
            
        </field>
    </record>

</odoo>