<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_master_hasil_view_form" model="ir.ui.view">
            <field name="name">res.master.hasil.view.form</field>
            <field name="model">res.master.hasil</field>
            <field name="arch" type="xml">
                <form string="Master Hasil">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="res_master_hasil_view_tree" model="ir.ui.view">
            <field name="name">res.master.hasil.view.tree</field>
            <field name="model">res.master.hasil</field>
            <field name="arch" type="xml">
                <tree string="Master Hasil">
                    <field name="name" /> 
                </tree>
            </field>
        </record>
        <record id="res_master_hasil_action" model="ir.actions.act_window">
            <field name="name">Master Master Hasil</field>
            <field name="res_model">res.master.hasil</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new master hasil
                    </p>
            </field>
        </record>

        <!-- <menuitem id="res_master_hasil_menu"
        parent="stock.menu_stock_config_settings"
        name="Master Hasil"
        action="jidoka_inventory.res_master_hasil_action"
        sequence="4"/> -->
    </data>
</odoo>
