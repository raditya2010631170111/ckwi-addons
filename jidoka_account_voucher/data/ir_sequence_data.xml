<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_voucher_default_in" model="ir.sequence">
            <field name="name">Number Voucher Default In</field>
            <field name="code">seq.voucher.default_in</field>
             <field name="suffix">/%(month)s/BKD</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_voucher_default_out" model="ir.sequence">
            <field name="name">Number Voucher Default Out</field>
            <field name="code">seq.voucher.default_out</field>
            <field name="suffix">/%(month)s/BKD</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_voucher_line" model="ir.sequence">
            <field name="name">Number Voucher Details</field>
            <field name="code">account.voucher.line</field>
            <field name="prefix">VL</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- <record id="seq_voucher_journal" model="ir.sequence">
            <field name="name">Number Voucher Journal Entries</field>
            <field name="code">account.voucher.journal</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record> -->
    </data>
</odoo>
